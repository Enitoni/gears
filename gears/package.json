    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^4.0.0",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.3.2",
    "@wessberg/rollup-plugin-ts": "^1.2.21",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "typedoc": "^0.14.2",
    "typescript": "^3.8.3"
  }
}
